{
  "expo": {
    "name": "Netflix",
    "slug": "netflix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Netflix-icon.png",
    "scheme": "myapp",
    "newArchEnabled": true,
    "userInterfaceStyle": "dark",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/netflix-adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.hassan546.netflix"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Netflix to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/Netflix-icon.png",
          "dark": {
            "image": "./assets/images/Netflix-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e79fdd23-a676-42ae-ac0d-625c0aafd189"
      }
    },
    "owner": "hassan546"
  }
}
